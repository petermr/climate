         </mfenced>
         <mo>+</mo>
         <msub>
          <mrow>
           <mi>q</mi>
          </mrow>
          <mrow>
           <mi mathvariant="normal">magma</mi>
          </mrow>
         </msub>
         <mfenced close=")" open="(" separators="">
          <mrow>
           <mi mathvariant="normal">Δ</mi>
           <mi>t</mi>
           <mfenced close=")" open="(" separators="">
            <mrow>
             <mi>i</mi>
            </mrow>
           </mfenced>
          </mrow>
