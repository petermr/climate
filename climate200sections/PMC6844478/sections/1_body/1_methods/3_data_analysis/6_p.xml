   <graphic xlink:href="pone.0225144.e003.jpg" id="pone.0225144.e003g" mimetype="image" position="anchor" orientation="portrait" xmlns:xlink="http://www.w3.org/1999/xlink"/>
   <math id="M3">
    <mrow>
     <msub>
      <mrow>
       <mi mathvariant="italic">CPUE</mi>
      </mrow>
      <mrow>
       <mi>y</mi>
       <mo>,</mo>
       <mi>r</mi>
      </mrow>
     </msub>
     <mo>=</mo>
     <mfrac>
      <mrow>
       <mstyle displaystyle="true">
        <mo>∑</mo>
        <mrow>
         <msub>
          <mrow>
           <mi>C</mi>
          </mrow>
          <mi>y</mi>
         </msub>
         <msub>
          <mrow/>
          <mrow>
