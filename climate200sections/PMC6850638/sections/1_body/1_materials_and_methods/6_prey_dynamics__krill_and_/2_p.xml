     <mrow>
      <mrow>
       <mi>L</mi>
       <mo>,</mo>
       <mi>y</mi>
      </mrow>
     </mrow>
     <mi>A</mi>
    </msubsup>
    <mspace width="0.166667em"/>
    <mn>0</mn>
    <mo>≤</mo>
    <mi>a</mi>
    <mo>≤</mo>
    <mi>z</mi>
    <mo>-</mo>
    <mn>2</mn>
   </mrow>
  </math>
 </disp-formula>
 <disp-formula id="gcb14573-disp-0003">
  <label>(1c)</label>
  <math id="nlm-math-4">
   <mrow>
    <msubsup>
     <mi>N</mi>
     <mrow>
      <mrow>
       <mi>L</mi>
       <mo>,</mo>
       <mi>y</mi>
       <mo>+</mo>
       <mn>1</mn>
       <mo>,</mo>
       <mi>z</mi>
      </mrow>
     </mrow>
     <mrow>
      <mi>k</mi>
      <mi>r</mi>
      <mi>i</mi>
      <mi>l</mi>
      <mi>l</mi>
      <mo>,</mo>
      <mi>A</mi>
     </mrow>
    </msubsup>
    <mo>=</mo>
    <mfenced close=")" open="(" separators="">
     <mrow>
      <msubsup>
       <mi>N</mi>
       <mrow>
        <mrow>
         <mi>L</mi>
         <mo>,</mo>
         <mi>y</mi>
         <mo>,</mo>
         <mi>z</mi>
        </mrow>
       </mrow>
       <mrow>
        <mi>k</mi>
        <mi>r</mi>
        <mi>i</mi>
        <mi>l</mi>
        <mi>l</mi>
        <mo>,</mo>
        <mi>A</mi>
       </mrow>
      </msubsup>
      <msup>
       <mi>e</mi>
       <mrow>
        <mo>-</mo>
        <mi>M</mi>
       </mrow>
      </msup>
      <mo>+</mo>
      <msubsup>
       <mi>N</mi>
       <mrow>
        <mrow>
         <mi>L</mi>
         <mo>,</mo>
         <mi>y</mi>
         <mo>,</mo>
         <mi>z</mi>
         <mo>-</mo>
         <mn>1</mn>
        </mrow>
       </mrow>
       <mrow>
        <mi>k</mi>
        <mi>r</mi>
        <mi>i</mi>
        <mi>l</mi>
        <mi>l</mi>
        <mo>,</mo>
        <mi>A</mi>
       </mrow>
      </msubsup>
      <msup>
