     <mi>N</mi>
     <mrow>
      <mrow>
       <mi>L</mi>
       <mo>,</mo>
       <mi>y</mi>
       <mo>+</mo>
       <mn>1</mn>
       <mo>,</mo>
       <mi>a</mi>
       <mo>+</mo>
       <mn>1</mn>
      </mrow>
     </mrow>
     <mrow>
      <mi>k</mi>
      <mi>r</mi>
      <mi>i</mi>
      <mi>l</mi>
      <mi>l</mi>
      <mo>,</mo>
      <mi>A</mi>
     </mrow>
    </msubsup>
    <mo>=</mo>
    <msubsup>
     <mi>N</mi>
