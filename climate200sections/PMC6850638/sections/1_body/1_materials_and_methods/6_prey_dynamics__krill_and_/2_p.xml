      <mi>k</mi>
      <mi>r</mi>
      <mi>i</mi>
      <mi>l</mi>
      <mi>l</mi>
      <mo>,</mo>
      <mi>A</mi>
     </mrow>
    </msubsup>
    <msup>
     <mi>e</mi>
     <mrow>
      <mo>-</mo>
