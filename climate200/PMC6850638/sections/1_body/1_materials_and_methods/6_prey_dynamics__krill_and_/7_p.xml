      <mi>A</mi>
     </mrow>
    </msubsup>
    <mo>=</mo>
    <munderover>
     <mo movablelimits="false">∑</mo>
     <mrow>
      <mrow>
       <mi>a</mi>
       <mo>=</mo>
       <mn>4</mn>
      </mrow>
     </mrow>
     <mi>z</mi>
    </munderover>
    <msubsup>
     <mi>w</mi>
     <mrow>
      <mrow>
       <mi>L</mi>
       <mo>,</mo>
