   <tex-math id="M17">\documentclass[12pt]{minimal} \usepackage{amsmath} \usepackage{wasysym} \usepackage{amsfonts} \usepackage{amssymb} \usepackage{amsbsy} \usepackage{mathrsfs} \usepackage{upgreek} \setlength{\oddsidemargin}{-69pt} \begin{document}$$\begin{array}{l}{\mathrm{P}}\left( {T_t = w|T_{t - {1}} = v} \right) = {\mathrm{P}}\left( {T_t = w,T_{t - {1}} = v} \right)/{\mathrm{P}}\left( {T_{t - {1}} = v} \right) = {\mathrm{R}}/{\mathrm{Q}}\\ \forall w,\;v \in {\mathbf{S}}^{\mathrm{T}}\end{array}$$\end{document}</tex-math>
